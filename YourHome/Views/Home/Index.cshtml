
@model List<Category>

<section class="header-bottom">


    <div class="container">
        <div class="row">

            <div class="title text-center">
                <h4>أقسام الموقع</h4>
                <p>سوف تجد ما تبحث عنه هنا</p>
            </div>


            @foreach (var item in Model)
            {
                <div class="col-md-6">
                    <a href="/Home/Countries/@item.Id/">
                        <img src="http://localhost:24548/upload/@item.Photo" style="width:90%;height:235px" />
                        <p class="project-cat">@item.Name</p>
                    </a>
                </div>
            }
            <div class="col-md-3">
            </div>


            <div class="col-md-6">
                <a href="/Home/AllIwaas/">
                    <img src="~/assets/images/02.jpg" style="width:90%;height:235px" />
                    <p class="project-cat">دار الايواء</p>
                </a>
            </div>

        </div>
    </div>


</section>
